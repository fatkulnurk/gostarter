# Application
APP_NAME=GoStarter
APP_VERSION=1.0.0

# Database
DB_USER=root
DB_PASSWORD=password
DB_HOST=mysql
DB_PORT=3306
DB_NAME=mydb
DB_PARAMS="charset=utf8mb4&parseTime=true"
DB_MAX_OPEN_CONNS=10
DB_MAX_IDLE_CONNS=5
DB_CONN_MAX_LIFETIME=1h
DB_CONN_MAX_IDLE_TIME=30m

# HTTP Server
HTTP_PREFORK=false
HTTP_CASE_SENSITIVE=true
HTTP_STRICT_ROUTING=false
HTTP_BODY_LIMIT=10485760
HTTP_SERVER_HEADER=GoStarter

# Queue
QUEUE_CONCURRENCY=10
QUEUE_WORKER_CONCURRENCY=10

# Redis
REDIS_ADDR=redis:6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_POOL_SIZE=10
REDIS_MIN_IDLE_CONNS=5
REDIS_CONN_MAX_LIFETIME=1h
REDIS_POOL_TIMEOUT=4s
REDIS_CONN_MAX_IDLE_TIME=30m
REDIS_READ_TIMEOUT=3s
REDIS_WRITE_TIMEOUT=3s
REDIS_DIAL_TIMEOUT=5s

# Schedule
SCHEDULE_TIMEZONE=UTC

# MAIL SMTP
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USERNAME=your_username
MAIL_PASSWORD=your_password
SMTP_AUTH_TYPE=PLAIN
SMTP_WITH_TLS_PORT_POLICY=0
